<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Canvas>
        <TextBox Name="hayStackTextBox" Width="290" Height="25" Canvas.Left="231" Canvas.Top="148"/>
        <ComboBox Name="actionsComboBox" SelectionChanged="ActionsComboBox_SelectionChanged"
                   Width="200" Height="25" Canvas.Left="152" Canvas.Top="210">
            <ComboBox.ItemTemplate>
                <DataTemplate>
                    <TextBlock Text="{Binding Description}"/>
                </DataTemplate>
            </ComboBox.ItemTemplate>
        </ComboBox>

        <Expander Background="Red" Header="Config" Name="expander" Width="200" Height="200">
            <Button Name="Process" Width="50" Height="25" Content="Process" Click="Process_Click" Canvas.Left="506" Canvas.Top="251"/>
        </Expander>
    
    </Canvas>
</Window>
